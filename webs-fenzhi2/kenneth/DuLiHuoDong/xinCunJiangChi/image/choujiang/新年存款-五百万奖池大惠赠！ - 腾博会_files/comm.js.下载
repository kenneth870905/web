//获取H5站点链接
function showH5Url() {
    var hostUrl = window.location.host;
    var h5url = '';
    if (hostUrl.indexOf('www') > -1)
        h5url = 'https://'+hostUrl.replace('www', 'm');
    else {
        h5url = 'https://m.' + hostUrl;
    }
    $("#gotoH5").show().attr('href', h5url);
    //var h5url = $.cookie('tengbo.com.h5url') || false,
    //    hosturl = window.location.host;
    //if (h5url) {
    //    if ((h5url.indexOf('vip') > -1 && hosturl.indexOf('vip') > -1) || (h5url.indexOf('vip') == -1 && hosturl.indexOf('vip') == -1)) {
    //        $("#gotoH5").show().attr('href', h5url);
    //        return;
    //    }
    //}

    //getH5Url(function (data) {
    //    if (data.row) {
    //        var h5url = data.row.H5_Url;
    //        if (hosturl.indexOf('vip') > -1) {
    //            h5url = data.row.VIPH5_Url;
    //        }
    //        $("#gotoH5").show().attr('href', h5url);
    //        $.cookie('tengbo.com.h5url', h5url, { path: '/' })
    //    }
    //})
}

//最近玩启动加载函数
function lastGamesInit() {
    if ($.cookie('memberACNT') && $.cookie('memberToken')) {
        getPlayGameHistory(function (data) {
            chanageTitle(data);
            if ($.cookie('showLastGames')) {
                $("#lastestGame-hide").parent().show();
                $("#showLastGames").hide();

            } else {
                $("#lastestGame-hide").parent().hide();
                $("#showLastGames").show();
            }
        });
    } else {
        $("#lastestGame-hide").parent().hide();
        $("#showLastGames").hide();
    }

    $("#lastestGame-hide").bind('click', hideLastGames);
    $("#showLastGames").click(function () {
        $.cookie('showLastGames', 'true', { path: '/' });
        var that = $(this);
        if (!$.cookie('memberACNT')) {
            getPlayGameHistory(function () {
                that.hide();
                $("#lastestGame-hide").parent().show();
            });
        } else {
            getPlayGameHistory(function () {
                that.hide();
                $("#lastestGame-hide").parent().show();
            });
        }
    })
}

//获取最近玩的电游
function getPlayGameHistory(fn) {
    $.invoke_api({ GetPlayGameHistory: {} }, {
        success: function (data) {
            //data = { Status: 1, row: [{ GameType: 17, GameCode: "4beauties", CreateTime: "" }, { GameType: 23, GameCode: "vs25pandagold", CreateTime: "" }], Summary: "" };
            if (data.Status == 1) {
                fn && fn(data.row);
                if (data.row && data.row.length) {
                    $("#lastestGame").html("");
                    $.each(data.row, function (i, o) {
                        if (o.GameType == 15) {
                            o.GameType = 35;
                        }
                        if (!o.IsMobile) {
                            var $li = $('<li class="fl"><div class="fl Pld-Eg-Ico"><img src=""></div><div class="bfc Pld-Eg-Text"><h2></h2><p></p></div></li>');
                            $li.find('img').attr('src', o.PCImgUrl);
                            //$li.find('h2').text(getGameName(o.GameType, o.GameCode));//GameName
                            $li.find('h2').html(o.GameName);
                            if (o.GameType == 20) {
                                $li.find('p').text(getFileName(o.GameType, 2) + '捕鱼');
                            } else {
                                $li.find('p').text(getFileName(o.GameType, 2) + '电游');
                            }

                            $li.click('click', function () {
                                var isH5 = o.IsHtml5 == 1 ? true : false;
                                playGame(null, false, o.GameCode, o.GameType, isH5, o.GameName);
                            })
                            $("#lastestGame").append($li);
                        }
                    })
                    $("#lastestGame").css({"padding":"0 50px"})
                    if (data.row.length < 11) {
                        $("#BoxUl_03 .tbbg-zz-l").hide();
                        $("#BoxUl_03 .tbbg-zz-r").hide();
                    } else {
                        $("#BoxUl_03 .tbbg-zz-l").show();
                        $("#BoxUl_03 .tbbg-zz-r").show();
                    }
                    if (lbMove && lbMove.timerId) return;
                    lbMove = LbMove('BoxUl_03', 'btnl_03', 'btnr_03', 'BoxSwitch', true, 'left', true, 180, 700, 5000, 10);
                }
            }
        }
    })

}

//根据ID获取平台名或平台图片文件夹 type 1，获取文件夹名     2，获取平台名
function getFileName(id, type) {
    type = type || 1;
    var plats = Util.GamePlat;
    if (plats[id]) {
        return type == 1 ? plats[id].imgPath : (plats[id].abbrCN || plats[id].abbr)
    }
    var fileName = { 9: "bbing", 12: "agg", 15: "ptg", 17: "mgg", 18: "ttgg", 21: "dtgg", 22: "swg", 23: "ppg", 24: "psg", 26: 'ptg', 20: 'gg', 27: 'aeg','30':'cq9g', 35:'ptg' };
    if (type == 2) {
        fileName = platformNames;
    }
    for (var obj in fileName) {
        if (obj == id) {
            return fileName[obj];
        }
    }
}

function hideLastGames() {
    $(this).parent().hide();
    $(".Tb-Played-Open").show();
    $.removeCookie("showLastGames", { path: '/' });
    //lbMove.clearTimer();
    //lbMove = null;
}

//切换显示 最近玩 和 热门电游 标题
function chanageTitle(data) {
    if (!data) return;
    var items = data[0];
    if (items.IsTouch != undefined) {
        if (!!items.IsTouch) {   //热门游戏
            $("#showLastGames div").attr('class', 'Tb-HotEgame-ico');
            $("#showLastGames span").text("热门电游");
            $("#lastestGame-hide").parent().find('.Tb-Pld-Eg-Bt i').text('热门电游');
        } else {
            $("#showLastGames div").attr('class', 'Tb-Played-ico');
            $("#showLastGames span").text('最近玩过');
            $("#lastestGame-hide").parent().find('.Tb-Pld-Eg-Bt i').text('我最近玩过');
        }
    } else {
        $("#showLastGames div").attr('class', 'Tb-Played-ico');
        $("#showLastGames span").text('最近玩过');
        $("#lastestGame-hide").parent().find('.Tb-Pld-Eg-Bt i').text('我最近玩过');
    }
}
