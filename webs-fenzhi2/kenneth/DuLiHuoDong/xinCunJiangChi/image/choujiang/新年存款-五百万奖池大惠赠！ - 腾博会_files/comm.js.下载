$(function () {
    if ($.cookie('memberToken') && (!$.cookie('ptlogin') || !$.cookie('ptinlogin') || !$.cookie('ptalogin'))) {
        egameLogin();
    }
    showH5Url();
    $("#login_member_status").remove();
    $('.navChange').navChange();
    $('#tb-topcontrol #tb-returntop').topControl();
    getAnno();
    $(".quest-but").questConponent();

    //顶部登录状态
    $(window).scroll(function () {
        $("#Live800_chatTip_iframe").hide();
    });

    if ($.cookie('memberToken')) {
        $(".tb-vipzxhb").hide();
        var groupId = $.cookie('memberGroupID');
        if (groupId && groupId > 3 && groupId<7) {
            $(".tb-vipzxhb").show();
        }
    }

    if (!!loginConponent) loginConponent.ShowState(".login_member_top_state", "#login_member_state_operate");
    showSubMenu();
    //悬浮点击半隐藏
    if (isMobile()) {
        $('.newyear-leftxuanfu .fixedClose,#tb-topcontrol .fixedClose').show();
        $('.newyear-leftxuanfu .fixedClose').click(function () {
            $(this).parent('.newyear-leftxuanfu').animate({ left: -120 }, 200).end().hide();
            $('.newyear-leftxuanfu .fixedExpland').show();
        })
        $('.newyear-leftxuanfu .fixedExpland').click(function () {
            $(this).parent('.newyear-leftxuanfu').animate({ left: 0 }, 200).end().hide();
            $('.newyear-leftxuanfu .fixedClose').show();
        })
        $('#tb-topcontrol .fixedClose').click(function () {
            $(this).parent('#tb-topcontrol').animate({ right: -120 }, 200).end().hide();
            $('#tb-topcontrol .fixedExpland').show();
        })
        $('#tb-topcontrol .fixedExpland').click(function () {
            $(this).parent('#tb-topcontrol').animate({ right: 0 }, 200).end().hide();
            $('#tb-topcontrol .fixedClose').show();
        })
    }

    setTimeout(function () {
        var navInfo = navigator.userAgent;
        if (navInfo.indexOf("MSIE 7") > -1 || navInfo.indexOf("MSIE 8") > -1 || navInfo.indexOf("MSIE 9") > -1 || navInfo.indexOf("MSIE 10") > -1) {
            //connNow();

            //$('body').Dialog({
            //    type: "Ask",
            //    title: "温馨提示",
            //    messageTitle: "你的IE浏览器不是最新版本,暂不支持消息推送，请升级到IE最新版本。",
            //    buttons: [{
            //        name: "确定",
            //        callback: function () {closeDialog(0);}
            //    }],
            //    closeHandler: function () { closeDialog(0); }
            //});
            //return;

        } else {
            watchStorageEvent();
            leavePage();
        }
        getUnreadMsgLen();
    }, 100);
    //最近玩游戏
    lastGamesInit();
    wathchRightSize();
});

